{
  "name": "pyxend-test",
  "displayName": "pyxend-test",
  "version": "0.0.1",
  "main": "extension.js",
  "activationEvents": [
    "onCommand:pyxendtest.get_selected_text",
    "onCommand:pyxendtest.get_cursor_pos",
    "onCommand:pyxendtest.get_language",
    "onCommand:pyxendtest.get_file_path",
    "onCommand:pyxendtest.get_all_text",
    "onCommand:pyxendtest.show_info_modal",
    "onCommand:pyxendtest.show_warn_modal",
    "onCommand:pyxendtest.show_error_modal",
    "onCommand:pyxendtest.replace_selected_text",
    "onCommand:pyxendtest.insert_text",
    "onCommand:pyxendtest.open_file",
    "onCommand:pyxendtest.set_cursor_pos",
    "onCommand:pyxendtest.save_file",
    "onCommand:pyxendtest.replace_all_text",
    "onCommand:pyxendtest.run_terminal_command"
  ],
  "contributes": {
    "commands": [
      {
        "command": "pyxendtest.get_selected_text",
        "title": "Get Selected Text"
      },
      {
        "command": "pyxendtest.get_cursor_pos",
        "title": "Get Cursor Pos"
      },
      {
        "command": "pyxendtest.get_language",
        "title": "Get Language"
      },
      {
        "command": "pyxendtest.get_file_path",
        "title": "Get File Path"
      },
      {
        "command": "pyxendtest.get_all_text",
        "title": "Get All Text"
      },
      {
        "command": "pyxendtest.show_info_modal",
        "title": "Show Info Modal"
      },
      {
        "command": "pyxendtest.show_warn_modal",
        "title": "Show Warn Modal"
      },
      {
        "command": "pyxendtest.show_error_modal",
        "title": "Show Error Modal"
      },
      {
        "command": "pyxendtest.replace_selected_text",
        "title": "Replace Selected Text"
      },
      {
        "command": "pyxendtest.insert_text",
        "title": "Insert Text"
      },
      {
        "command": "pyxendtest.open_file",
        "title": "Open File"
      },
      {
        "command": "pyxendtest.set_cursor_pos",
        "title": "Set Cursor Pos"
      },
      {
        "command": "pyxendtest.save_file",
        "title": "Save File"
      },
      {
        "command": "pyxendtest.replace_all_text",
        "title": "Replace All Text"
      },
      {
        "command": "pyxendtest.run_terminal_command",
        "title": "Run Terminal Command"
      }
    ]
  },
  "engines": {
    "vscode": "^1.75.0"
  }
}